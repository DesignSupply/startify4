{
  "name": "startify4",
  "version": "1.0.0",
  "description": "Development environment for website creation with design and static coding.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-vue": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:html",
    "lint:style": "stylelint src/**/*.{css,scss,vue,ts,tsx}",
    "lint:es": "eslint src/**/*.{vue,ts,tsx}",
    "lint:html": "markuplint src/**/*.{html,hbs,vue,pug,tsx}",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md,html,vue,hbs}' 'dist/**/*.html'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesignSupply/startify4.git"
  },
  "bugs": {
    "url": "https://github.com/DesignSupply/startify4/issues",
    "email": "info@designsuply-web.com"
  },
  "author": {
    "name": "Ogawa Shinya",
    "email": "info@designsuply-web.com",
    "url": "https://designsupply-web.com/"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.7.0",
    "@markuplint/mustache-parser": "^3.6.1",
    "@markuplint/pug-parser": "^3.7.0",
    "@markuplint/react-spec": "^3.8.0",
    "@markuplint/vue-parser": "^3.6.1",
    "@markuplint/vue-spec": "^3.7.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/jquery": "^3.5.16",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "css-declaration-sorter": "^6.4.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-vue": "^9.11.0",
    "globule": "^1.3.4",
    "markuplint": "^3.8.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "sass": "^1.60.0",
    "stylelint": "^15.5.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.3.9",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-pug": "^0.3.2",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "jquery": "^3.6.4",
    "pinia": "^2.0.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "browserslist": [
    "last 2 version",
    "ie >= 11",
    "iOS >= 8.1",
    "Android >= 4.4"
  ],
  "resolutions": {
    "@types/react": "18.2.0"
  }
