{
  "name": "startify4",
  "version": "1.0.2",
  "description": "Development environment for website creation with design and static coding.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-vue": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:html",
    "lint:style": "stylelint src/**/*.{css,scss,vue,ts,tsx}",
    "lint:es": "eslint src/**/*.{vue,ts,tsx}",
    "lint:html": "markuplint src/**/*.{html,hbs,vue,pug,tsx}",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md,html,vue,hbs}' 'dist/**/*.html'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesignSupply/startify4.git"
  },
  "bugs": {
    "url": "https://github.com/DesignSupply/startify4/issues",
    "email": "info@designsuply-web.com"
  },
  "author": {
    "name": "Ogawa Shinya",
    "email": "info@designsuply-web.com",
    "url": "https://designsupply-web.com/"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.12.0",
    "@markuplint/mustache-parser": "^3.12.0",
    "@markuplint/pug-parser": "^3.12.0",
    "@markuplint/react-spec": "^3.13.0",
    "@markuplint/vue-parser": "^3.12.0",
    "@markuplint/vue-spec": "^3.13.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/globule": "^1.1.9",
    "@types/jquery": "^3.5.29",
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.19",
    "css-declaration-sorter": "^6.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-vue": "^9.25.0",
    "globule": "^1.3.4",
    "markuplint": "^3.15.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "sass": "^1.75.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-pug": "^0.3.2",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "pinia": "^2.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2"
  },
  "browserslist": [
    "last 2 version",
    "ie >= 11",
    "iOS >= 8.1",
    "Android >= 4.4"
  ]
}
